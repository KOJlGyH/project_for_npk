  <property name="styleSheet">
   <string notr="true">/* General Settings */
QWidget {
    font-family: &quot;Comic Sans MS&quot;; /* Fun and playful font */
    font-size: 14px;
    color: #2c3e50; /* Dark blue for text */
    background-color: #f9f9f9; /* Light gray background */
}

/* Background Images for Different Pages */
QWidget#mathPage {
    background-image: url(:/backgrounds/math_background.png); /* Math-themed background */
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

QWidget#sciencePage {
    background-image: url(:/backgrounds/science_background.png); /* Science-themed background */
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

QWidget#literaturePage {
    background-image: url(:/backgrounds/literature_background.png); /* Literature-themed background */
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

QWidget#historyPage {
    background-image: url(:/backgrounds/history_background.png); /* History-themed background */
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

/* Buttons */
QPushButton {
    background-color: #ff6f61; /* Coral */
    color: white;
    border: 2px solid #ff3b2f; /* Darker coral */
    border-radius: 10px;
    padding: 10px 20px;
    font-weight: bold;
    font-size: 16px;
}

QPushButton:hover {
    background-color: #ff3b2f; /* Darker coral on hover */
}

QPushButton:pressed {
    background-color: #ff1a1a; /* Even darker coral when pressed */
}

QPushButton:disabled {
    background-color: #bdc3c7; /* Gray */
    color: #7f8c8d; /* Light gray text */
    border: 2px solid #95a5a6;
}

/* Line Edits */
QLineEdit {
    background-color: rgba(255, 255, 255, 0.9); /* Semi-transparent white */
    border: 2px solid #3498db; /* Blue border */
    border-radius: 10px;
    padding: 10px;
    font-size: 14px;
}

QLineEdit:focus {
    border: 2px solid #ff6f61; /* Coral border when focused */
}

/* Labels */
QLabel {
    color: #2c3e50; /* Dark blue */
    font-size: 14px;
    background-color: rgba(255, 255, 255, 0.8); /* Semi-transparent white */
    border-radius: 10px;
    padding: 10px;
}

QLabel#titleLabel {
    font-size: 24px;
    font-weight: bold;
    color: #3498db; /* Blue for titles */
    background-color: transparent; /* No background for titles */
}

/* Combo Boxes */
QComboBox {
    background-color: rgba(255, 255, 255, 0.9); /* Semi-transparent white */
    border: 2px solid #3498db; /* Blue border */
    border-radius: 10px;
    padding: 10px;
    font-size: 14px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left: 2px solid #3498db;
}

QComboBox::down-arrow {
    image: url(:/icons/down_arrow.png); /* Replace with your icon */
}

QComboBox QAbstractItemView {
    background-color: white;
    border: 2px solid #3498db;
    selection-background-color: #ff6f61; /* Coral for selected item */
    selection-color: white;
}

/* Check Boxes */
QCheckBox {
    color: #2c3e50;
    background-color: rgba(255, 255, 255, 0.8); /* Semi-transparent white */
    border-radius: 10px;
    padding: 10px;
}

QCheckBox::indicator {
    width: 20px;
    height: 20px;
}

QCheckBox::indicator:checked {
    image: url(:/icons/checked.png); /* Replace with your icon */
}

QCheckBox::indicator:unchecked {
    image: url(:/icons/unchecked.png); /* Replace with your icon */
}

/* Radio Buttons */
QRadioButton {
    color: #2c3e50;
    background-color: rgba(255, 255, 255, 0.8); /* Semi-transparent white */
    border-radius: 10px;
    padding: 10px;
}

QRadioButton::indicator {
    width: 20px;
    height: 20px;
}

QRadioButton::indicator:checked {
    image: url(:/icons/radio_checked.png); /* Replace with your icon */
}

QRadioButton::indicator:unchecked {
    image: url(:/icons/radio_unchecked.png); /* Replace with your icon */
}

/* Progress Bars */
QProgressBar {
    background-color: #bdc3c7;
    border: 2px solid #95a5a6;
    border-radius: 10px;
    text-align: center;
    color: #2c3e50;
}

QProgressBar::chunk {
    background-color: #ff6f61; /* Coral */
    border-radius: 10px;
}

/* Spin Boxes */
QSpinBox {
    background-color: rgba(255, 255, 255, 0.9); /* Semi-transparent white */
    border: 2px solid #3498db; /* Blue border */
    border-radius: 10px;
    padding: 10px;
    font-size: 14px;
}

QSpinBox::up-button, QSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 30px;
    border-left: 2px solid #3498db;
}

QSpinBox::up-arrow {
    image: url(:/icons/up_arrow.png); /* Replace with your icon */
}

QSpinBox::down-arrow {
    image: url(:/icons/down_arrow.png); /* Replace with your icon */
}

/* Group Boxes */
QGroupBox {
    border: 2px solid #3498db; /* Blue border */
    border-radius: 10px;
    margin-top: 10px;
    padding-top: 15px;
    font-weight: bold;
    color: #3498db; /* Blue for group box titles */
    background-color: rgba(255, 255, 255, 0.8); /* Semi-transparent white */
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 10px;
}

/* Tabs */
QTabWidget::pane {
    border: 2px solid #3498db; /* Blue border */
    border-radius: 10px;
    padding: 10px;
    background-color: rgba(255, 255, 255, 0.9); /* Semi-transparent white */
}

QTabBar::tab {
    background-color: #ecf0f1;
    border: 2px solid #3498db;
    border-bottom: none;
    padding: 10px 20px;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    font-size: 14px;
}

QTabBar::tab:selected {
    background-color: white;
    border-bottom: 2px solid white;
}

QTabBar::tab:hover {
    background-color: #ff6f61; /* Coral on hover */
    color: white;
}

/* Scroll Bars */
QScrollBar:horizontal, QScrollBar:vertical {
    background-color: #ecf0f1;
    border-radius: 10px;
}

QScrollBar::handle:horizontal, QScrollBar::handle:vertical {
    background-color: #ff6f61; /* Coral */
    border-radius: 10px;
}

QScrollBar::add-line, QScrollBar::sub-line {
    background: none;
}

/* Tool Tips */
QToolTip {
    background-color: #ff6f61; /* Coral */
    color: white;
    border: 2px solid #ff3b2f;
    padding: 10px;
    border-radius: 10px;
}

/* Menus */
QMenuBar {
    background-color: #ecf0f1;
    color: #2c3e50;
}

QMenuBar::item {
    padding: 10px 20px;
}

QMenuBar::item:selected {
    background-color: #ff6f61; /* Coral */
    color: white;
}

QMenu {
    background-color: white;
    border: 2px solid #3498db;
    padding: 10px;
}

QMenu::item {
    padding: 10px 20px;
}

QMenu::item:selected {
    background-color: #ff6f61; /* Coral */
    color: white;
}

/* Tables */
QTableView {
    background-color: white;
    border: 2px solid #3498db;
    border-radius: 10px;
}

QTableView::item {
    padding: 10px;
}

QTableView::item:selected {
    background-color: #ff6f61; /* Coral */
    color: white;
}

/* Text Edit */
QTextEdit {
    background-color: rgba(255, 255, 255, 0.9); /* Semi-transparent white */
    border: 2px solid #3498db;
    border-radius: 10px;
    padding: 10px;
}

QTextEdit:focus {
    border: 2px solid #ff6f61; /* Coral */
}

/* Status Bar */
QStatusBar {
    background-color: #ecf0f1;
    border-top: 2px solid #3498db;
    padding: 10px;
}

/* Tool Bars */
QToolBar {
    background-color: #ecf0f1;
    border-bottom: 2px solid #3498db;
    padding: 10px;
}

QToolBar::separator {
    background-color: #3498db;
    width: 2px;
    margin: 10px;
}</string>
